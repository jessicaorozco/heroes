{
    "rules": {
      "no-console": false,
      "no-empty": false,
      "no-trailing-whitespace": true,
      "quotemark": [
        true,
        "single"
      ],
      "semi": [
        true,
        "always"
      ],
      "triple-equals": true,
      "typedef": [
        true,
        "property-declaration",
        "variable-declaration",
        "member-variable-declaration"
      ],
      "arrow-parens": false,
      "class-name": true,
      "interface-name": true,
      "variable-name": [
        true,
        "camelCase"
      ],
      "max-classes-per-file": 1,
      "max-line-length": 140
    }
  }
  